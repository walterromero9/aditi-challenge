{
    "include": [
        "./src/**/*"
    ],
    "compilerOptions": {
        "target": "ESNext",
        "lib": ["DOM", "DOM.Iterable", "ESNext"],
        "skipLibCheck": true,
        "esModuleInterop": true,
        "strict": true,
        "module": "ESNext",
        "moduleResolution": "Node",
        "isolatedModules": true,
        "noEmit": true,
        "jsx": "react-jsx"
    }
}